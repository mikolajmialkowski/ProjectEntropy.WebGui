entity Cryptocurrency {
	name String,
    pair String,
    symbol String,
    price String,
}

entity Prediction {
    dateFrom String,
    dateTo String,
    duration String, 
    currentPrice String,
    predictedPrice String,
    probability String,
}

entity SchedulerSettings {
	interval String,
    limit String,
}

entity ApiSettings {
	apiUri1 String,
    apiUri2 String,
    apiUri3 String,
    apiToken String,
}

entity AiSettings {
}

entity GeneralInfo{
	recordsInDataBaseAmount BigDecimal,
    apiCallsAmount BigDecimal,
    apiStatistics String,
}

relationship OneToMany {
	Cryptocurrency to Prediction {cryptocurrency},
}








